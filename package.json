{
  "name": "nodebase-workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "turbo run format",
    "dev:web": "pnpm --filter @nodebase/web dev",
    "build:web": "pnpm --filter @nodebase/web build",
    "db:push": "tsx node_modules/prisma/build/index.js db push",
    "db:generate": "tsx node_modules/prisma/build/index.js generate",
    "db:seed": "tsx prisma/seed-templates.ts",
    "redis:start": "docker start redis-nodebase || docker run -d --name redis-nodebase -p 6379:6379 redis:7-alpine",
    "redis:stop": "docker stop redis-nodebase",
    "redis:restart": "docker restart redis-nodebase",
    "redis:logs": "docker logs -f redis-nodebase",
    "redis:cli": "docker exec -it redis-nodebase redis-cli",
    "inngest:dev": "inngest-cli dev",
    "dev:all": "dotenv -- mprocs"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "dotenv-cli": "^11.0.0",
    "inngest-cli": "^1.12.1",
    "mprocs": "^0.7.3",
    "prisma": "^6.16.3",
    "tsx": "^4.20.6",
    "turbo": "^2.3.0",
    "typescript": "^5"
  },
  "overrides": {
    "import-in-the-middle": "2.0.4",
    "require-in-the-middle": "8.0.1"
  },
  "packageManager": "pnpm@9.15.0"
}
